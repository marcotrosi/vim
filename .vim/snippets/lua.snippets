snippet lua
	#!/usr/local/bin/lua
snippet ip
	for ${2:i},${3:v} in ipairs(${1:t}) do
		$4
	end
snippet p
	for ${2:k},${3:v} in pairs(${1:t}) do
		$4
	end
snippet for
	for ${1:i}=1,${2:10} do
	end
snippet df
	${2:t} = dofile(${1:file})$3
snippet ti
	table.insert(${1:t}, ${2:line})$3
snippet tr
	table.remove(${1:t}, ${2:1})$3
snippet tc
	table.concat(${1:t}, ${2:sep})$3
snippet tm
	table.maxn(${1:t})$3
snippet pri
	print("$1")
	
